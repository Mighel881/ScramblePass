<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>General</string>
			<key>footerText</key>
			<string>The scramble trigger determines when the buttons will be rescrambled. If manually is selected, then they will not be scrambled unless you press the scramble button above.

You must have a passcode set in Settings > General > Passcode Lock.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true />
			<key>defaults</key>
			<string>com.expetelek.scramblepasspreferences</string>
			<key>key</key>
			<string>isEnabled</string>
			<key>label</key>
			<string>Enabled</string>
			<key>PostNotification</key>
			<string>com.expetelek.scramblepass/settingsChanged</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSLinkListCell</string>
			<key>default</key>
			<integer>0</integer>
			<key>defaults</key>
			<string>com.expetelek.scramblepasspreferences</string>
			<key>key</key>
			<string>scrambleTrigger</string>
			<key>label</key>
			<string>Scramble Trigger</string>
			<key>detail</key>
			<string>PSListItemsController</string>
			<key>validValues</key>
			<array>
				<integer>0</integer>
				<integer>1</integer>
			</array>
			<key>validTitles</key>
			<array>
				<string>Every Unlock</string>
				<string>Manually</string>
			</array>
			<key>PostNotification</key>
			<string>com.expetelek.scramblepass/settingsChanged</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Scramble Buttons</string>
			<key>action</key>
			<string>scrambleButtons:</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Links</string>
		</dict>
		<dict>
			<key>icon</key>
			<string>twitter.png</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>@hetelek</string>
			<key>action</key>
			<string>openTwitter:</string>
		</dict>
		<dict>
			<key>icon</key>
			<string>github.png</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>View Source</string>
			<key>action</key>
			<string>openGithub:</string>
		</dict>
	</array>
	<key>title</key>
	<string>ScramblePass</string>
</dict>
</plist>
